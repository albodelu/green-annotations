<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.tmtron.greenannotations.test"
          android:versionCode="1"
          android:versionName="1.0.0-SNAPSHOT">

    <uses-sdk
            android:minSdkVersion="17"
            android:targetSdkVersion="17" tools:ignore="OldTargetApi"/>

    <!--
        tools:ignore="all"
        does not suppress the “activity is not registered in the manifest” lint-warning
        http://stackoverflow.com/questions/39590817/androidannotations-how-to-handle-activity-is-not-registered-in-the-manifest
      -->
    <application
        android:label="@string/app_name"
        android:allowBackup="false"
        android:icon="@android:drawable/alert_dark_frame"
        tools:ignore="all">

        <activity android:name=".GreenEventBusActivity_" />
    </application>

</manifest>
